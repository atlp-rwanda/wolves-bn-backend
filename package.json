    "test": "npm run createtables && npm run seed:test && cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register  ./test/*.test.js --timeout 10000 --exit || true && npm run droptables",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "createtables": "cross-env NODE_ENV=test sequelize db:migrate",
    "droptables": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all"
